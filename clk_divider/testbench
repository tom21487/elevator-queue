#! /usr/local/bin/vvp
:ivl_version "12.0 (devel)" "(s20150603-1148-gef01dd1e)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x118dd50 .scope module, "testbench" "testbench" 2 13;
 .timescale -9 -11;
P_0x11b3cc0 .param/l "DURATION" 1 2 22, +C4<00000000000000000010011100010000>;
P_0x11b3d00 .param/real "HALF_PERIOD" 1 2 23, Cr<m53570a3d70a3d800gfc7>; value=41.6700
v0x11de900_0 .var "clk", 0 0;
v0x11de9a0_0 .net "clk_divided", 0 0, v0x11de500_0;  1 drivers
v0x11dea70_0 .var "rst", 0 0;
S_0x118df80 .scope module, "uut" "clk_divider" 2 37, 3 1 0, S_0x118dd50;
 .timescale -9 -11;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 1 "clk_divided";
P_0x118e160 .param/l "MAX_CLK_CNT" 0 3 7, +C4<00000000000000000000000000000010>;
v0x11c9110_0 .net "clk", 0 0, v0x11de900_0;  1 drivers
v0x11de420_0 .var "clk_cnt", 0 0;
v0x11de500_0 .var "clk_divided", 0 0;
v0x11de5d0_0 .var "next_clk_cnt", 0 0;
v0x11de6b0_0 .var "next_clk_divided", 0 0;
v0x11de7c0_0 .net "rst", 0 0, v0x11dea70_0;  1 drivers
E_0x11c9ca0 .event posedge, v0x11c9110_0;
E_0x11ca830 .event anyedge, v0x11de420_0, v0x11de500_0;
    .scope S_0x118df80;
T_0 ;
    %wait E_0x11ca830;
    %load/vec4 v0x11de420_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x11de500_0;
    %inv;
    %store/vec4 v0x11de6b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11de5d0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x11de500_0;
    %store/vec4 v0x11de6b0_0, 0, 1;
    %load/vec4 v0x11de420_0;
    %pad/u 2;
    %addi 1, 0, 2;
    %pad/u 1;
    %store/vec4 v0x11de5d0_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x118df80;
T_1 ;
    %wait E_0x11c9ca0;
    %load/vec4 v0x11de7c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x11de500_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x11de420_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x11de6b0_0;
    %assign/vec4 v0x11de500_0, 0;
    %load/vec4 v0x11de5d0_0;
    %assign/vec4 v0x11de420_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x118dd50;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11de900_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11dea70_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x118dd50;
T_3 ;
    %delay 4167, 0;
    %load/vec4 v0x11de900_0;
    %inv;
    %store/vec4 v0x11de900_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x118dd50;
T_4 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x11dea70_0, 0, 1;
    %delay 4167, 0;
    %delay 4167, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11dea70_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0x118dd50;
T_5 ;
    %vpi_call 2 55 "$dumpfile", "testbench.vcd" {0 0 0};
    %vpi_call 2 56 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x118dd50 {0 0 0};
    %delay 1000000, 0;
    %vpi_call 2 62 "$display", "Finished!" {0 0 0};
    %vpi_call 2 63 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
    "clk_divider.v";
